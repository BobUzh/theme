<div class="container">
  {% for block in section.blocks %}
    <a href="{{ block.settings.link }}">{{ block.settings.title }}</a>
  {% endfor %}
</div>

{% schema %}
  {
    "name": "Menu page Tabs scroll",
    "class": "menu-page-tabs-scroll",
    
    "settings": [
       
    ],
    "blocks": [
		{
			"type": "item",
			"name": "item",
			"settings": [
              {
                 "type":"text",
                 "id":"title",
                 "label":"Title"
               },
               {
                  "type": "url",
                  "id": "link",
                  "label": "Link"
              }
			]
		}
	],
     "presets": [
    {
      "name": "Menu page Tabs scroll"
    }
  ]
  }
{% endschema %}